<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.finalproject.presentation.views.SearchAuthor">
  <grid id="searchAuthorPanel" binding="mainPanel" layout-manager="GridLayoutManager" row-count="3" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="10" vgap="5">
    <margin top="10" left="10" bottom="10" right="10"/>
    <constraints>
      <xy x="20" y="20" width="600" height="300"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="labelAuthorName" class="javax.swing.JLabel">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0"/>
        </constraints>
        <properties>
          <text value="Author Name:"/>
        </properties>
      </component>
      <component id="authorNameField" class="javax.swing.JTextField" binding="authorNameField">
        <constraints>
          <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="2" anchor="8" fill="1"/>
        </constraints>
      </component>
      <component id="searchButton" class="javax.swing.JButton" binding="searchButton">
        <constraints>
          <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="4" fill="0"/>
        </constraints>
        <properties>
          <text value="Search"/>
        </properties>
      </component>
      <scrollpane id="authorScrollPane" class="javax.swing.JScrollPane">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="2" vsize-policy="6" hsize-policy="6" anchor="0" fill="3"/>
        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <component id="authorTable" class="javax.swing.JTable" binding="authorTable">
            <constraints/>
            <properties/>
          </component>
        </children>
      </scrollpane>
    </children>
  </grid>
</form> 